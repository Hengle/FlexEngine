func recurse(int val, int depth) -> int {
  if (depth > 0) {
    return val * recurse(val, depth - 1);
  }
  return val;
}

func do_math(float val0, int add, float mul)
  -> float {
  float result = val0;
  result += (float)add;
  result *= mul;
  return result;
}

func square(int num) -> int {
  return num * num;
}

func double(int num) -> int {
  return num * 2;
}

